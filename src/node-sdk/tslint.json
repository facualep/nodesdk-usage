{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [100]
    },
    "semicolon": [true, "never"],
    "quotemark": [true, "single"],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-console": true,
    "radix": false,
    "no-consecutive-blank-lines": [true, 2],
    "arrow-parens": false,
    "triple-equals": false,
    "no-namespace": false,
    "prefer-for-of": false
  }
}
